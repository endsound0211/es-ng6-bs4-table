{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            ["app", "es"],
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            ["app", "es"],
            "kebab-case"
        ],
        "max-line-length": [false],
        "member-ordering": [false],
        "no-output-on-prefix": [false]
    }
}
